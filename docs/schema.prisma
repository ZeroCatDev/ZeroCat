generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model oauth_applications {
  id            Int      @id(map: "idx_16442_primary")
  owner_id      Int
  name          String   @db.VarChar(191)
  description   String?  @db.VarChar(191)
  homepage_url  String?  @db.VarChar(191)
  client_id     String   @unique(map: "idx_16442_oauth_applications_client_id_key") @db.VarChar(191)
  client_secret String   @db.VarChar(191)
  redirect_uris Json     @db.Json
  type          String   @default("oauth") @db.VarChar(191)
  client_type   String   @default("confidential") @db.VarChar(191)
  scopes        Json     @db.Json
  webhook_url   String?  @db.VarChar(191)
  logo_url      String?  @db.VarChar(191)
  terms_url     String?  @db.VarChar(191)
  privacy_url   String?  @db.VarChar(191)
  status        String   @default("active") @db.VarChar(191)
  is_verified   Boolean  @default(false)
  is_public     Boolean  @default(false)
  created_at    DateTime @default(dbgenerated("timezone('Asia/Shanghai', now())")) @db.Timestamptz(6)
  updated_at    DateTime @updatedAt @db.Timestamptz(6)

  @@index([client_id], map: "idx_16442_oauth_applications_client_id_idx")
  @@index([owner_id], map: "idx_16442_oauth_applications_owner_id_idx")
}

